<Window x:Class="PracticProject.Forms.EditRabForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditRabForm" Height="160" Width="397" Icon="..\Images\WinIcon.ico">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\Styles\Brushes.xaml"/>
                <ResourceDictionary Source="..\Styles\Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Rectangle 
            RadiusX="3"
            RadiusY="3"
            Fill="{StaticResource TreeViewStyleBackGround}">
        </Rectangle>
        <TextBlock Text="Измените работу" VerticalAlignment="Center" FontSize="15" Margin="112,18,112,79" Height="24" />
        <TextBox Name="TextBoxRab" Width="200" TextAlignment="Center" FontSize="20"  Margin="88,43,88,49" />
        <Button Name="Button_Ok" Content="Ok" Height="25" Width="70"
                HorizontalAlignment="Center"   Margin="39,75,266,21" Click="Button_Ok_Click"/>
        <Button Name="Button_Cancel" Content="Отмена" Height="25" Width="70"
                HorizontalAlignment="Center"   Margin="266,75,39,21" Click="Button_Cancel_Click"/>
        
    </Grid>
</Window>
